<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="245.0" prefWidth="316.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.controllers.MyProfileFormController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <Label text="Мой профиль">
    </Label>

    <HBox spacing="10">
        <Label text="Имя пользователя:" />
        <Label fx:id="usernameLabel" />
    </HBox>

    <HBox spacing="10">
        <Label text="Роль:" />
        <Label fx:id="roleLabel" />
    </HBox>

    <HBox spacing="10">
        <Label text="Email:" />
        <Label fx:id="emailLabel" />
    </HBox>


    <Button text="Редактировать профиль" />

    <Button text="Назад" />
</VBox>
